{

  "description": "Phishing Link detector",
  "manifest_version": 2,
  "name": "ThunderbirdDetector",
  "version": "1.0",
  "icons": {
    "48": "icons/message.svg"
  },

  "applications": {
    "gecko": {
      "id": "ping_pong@example.org",
      "strict_min_version": "50.0"
    }
  },  
  "message_display_action": {
    "default_popup": "messagePopup/popup.html",
    "default_title": "URLCheck",
    "default_icon": "icons/message.svg"
  },

  "permissions": ["nativeMessaging","messagesRead"]

}
